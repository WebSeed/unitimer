{
  "name": "unitimer",
  "version": "3.11.0",
  "description": "Universal timer (Node.js and browser)",
  "source": "src/index.js",
  "main": "lib/unitimer.js",
  "umd:main": "lib/unitimer.umd.js",
  "module": "lib/unitimer.m.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "lint": "eslint ./src --ext .js",
    "fix": "eslint ./src --ext .js --fix",
    "unit": "ava -v",
    "test": "yarn lint && ava -v"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "lib/**/*.js"
    ]
  },
  "standard": {
    "ignore": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebSeed/unitimer.git"
  },
  "keywords": [
    "timer",
    "profile",
    "performance",
    "measure",
    "speed",
    "console.time",
    "hrtime",
    "Date.now"
  ],
  "author": "Will Bamford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WebSeed/unitimer/issues"
  },
  "homepage": "https://github.com/WebSeed/unitimer#readme",
  "devDependencies": {
    "ava": "^1.4.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.2",
    "microbundle": "^0.11.0",
    "proxyquire": "^2.1.3",
    "sinon": "^7.4.2",
    "watchify": "^3.7.0"
  }
}
